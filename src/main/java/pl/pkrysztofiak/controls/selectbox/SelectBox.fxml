<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>

<HBox styleClass="select-box" stylesheets="@SelectBox.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane styleClass="item-cell" HBox.hgrow="ALWAYS">
         <children>
            <Label text="Jetbrains Mono" />
         </children></StackPane>
      <StackPane fx:id="arrowButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="arrow-button">
         <children>
            <Region maxHeight="-Infinity" maxWidth="-Infinity" styleClass="arrow" />
         </children>
      </StackPane>
   </children>
   <padding>
      <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
   </padding>
</HBox>
